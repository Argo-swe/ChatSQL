{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build-api": "npx openapicmd typegen http://localhost:8000/openapi.json > src/types/openapi.d.ts",
    "precommit": "npm run lint && npm build && npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ../setup-pre-commit.sh && pre-commit run --all-files && npm run precommit"
    }
  },
  "dependencies": {
    "axios": "~1.7.2",
    "jwt-decode": "~4.0.0",
    "openapi-client-axios": "~7.5.5",
    "primeflex": "~3.3.1",
    "primeicons": "~7.0.0",
    "scss": "~0.2.4",
    "vue": "~3.4.21",
    "vue-i18n": "~9.13.1",
    "vue-router": "~4.3.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "~2.13.4",
    "@rushstack/eslint-patch": "~1.8.0",
    "@tsconfig/node20": "~20.1.4",
    "@types/node": "~20.12.5",
    "@vitejs/plugin-vue": "~5.0.4",
    "@vue/eslint-config-prettier": "~9.0.0",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/tsconfig": "~0.5.1",
    "eslint": "~8.57.0",
    "eslint-plugin-vue": "~9.23.0",
    "husky": "~4.3.8",
    "node-wget": "~0.4.3",
    "npm-run-all2": "~6.1.2",
    "prettier": "~3.2.5",
    "prettier-plugin-organize-imports": "^4.0.0",
    "primevue": "~3.52.0",
    "sass": "~1.77.5",
    "typescript": "~5.4.0",
    "vite": "~5.2.8",
    "vue-tsc": "~2.0.11",
    "xml-parser": "~1.2.1"
  }
}
